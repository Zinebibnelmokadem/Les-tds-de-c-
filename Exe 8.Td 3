#include <iostream>
using namespace std;

template<typename T> T carre (T x){
     T c=x*x;
     return c;
     
 }
main(){
 int n;
 float m;
 cout<<"entrer la valeur de entier n :";
 cin>>n; 
 cout<<"entrer la valeur de entier m :";
 cin>>m; 
 cout<<"la caree de cette nombre n est :"<<carre(n)<<endl;
 cout<<"la caree de cette nombre m est :"<<carre(m)<<endl;

}
